<% cache @podcasts.first do %>
<div class="col-md-3 col-md-offset-1 recent-podcasts">	
	<h4>Recent Podcasts:</h4>
		<ul class="updates">
			<% @podcasts.each do |podcast|%>
			<li class="recent_podcast podcast_item" data-id="<%= podcast.id %>">
			    <p><%= podcast.title %></p>
			</li>
			<% end %>
		</ul>
</div>
<% end %>